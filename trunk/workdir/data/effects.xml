<effects>   
	<!-- FORWARD -->
	<effect name="PositionNormalTechniqueForward" shader_family="PositionNormalForward">
   <shader stage="vertex"   name="PositionNormalForward"/>
   <shader stage="pixel"    name="PositionNormalForward"/>
  </effect>
  <effect name="PositionNormalUVTechniqueForward" shader_family="PositionNormalUVForward">
   <shader stage="vertex"   name="PositionNormalUVForward"/>
   <shader stage="pixel"    name="PositionNormalUVForward"/>
  </effect>
  <effect name="PositionNormalUVUV2TechniqueForward" shader_family="PositionNormalUVUV2Forward">
   <shader stage="vertex"   name="PositionNormalUVUV2Forward"/>
   <shader stage="pixel"    name="PositionNormalUVUV2Forward"/>
  </effect>
  <effect name="PositionBumpUVTechniqueForward" shader_family="PositionBumpUVForward">
   <shader stage="vertex"   name="PositionBumpUVForward"/>
   <shader stage="pixel"    name="PositionBumpUVForward"/>
  </effect>
  <effect name="AnimatedModelTechniqueForward"  shader_family="AnimatedModelForward">
	<shader stage="vertex" name="AnimatedModelForward"/>
	<shader stage="pixel" name="AnimatedModelForward"/>
  </effect>
  
<!--  <effect name="EnvironmentMapTechniqueForward" shader_family="EnvironmentMapForward">
   <shader stage="vertex"   name="EnvironmentMapForward"/>
   <shader stage="pixel"    name="EnvironmentMapForward"/>
  </effect>-->

  
  <!-- DEFERRED -->
  <effect name="PositionNormalTechniqueDeferred" shader_family="PositionNormalDeferred">
   <shader stage="vertex"   name="PositionNormalDeferred"/>
   <shader stage="pixel"    name="PositionNormalDeferred"/>
  </effect>
   <effect name="PositionNormalUVTechniqueDeferred" shader_family="PositionNormalUVDeferred">
   <shader stage="vertex"   name="PositionNormalUVDeferred"/>
   <shader stage="pixel"    name="PositionNormalUVDeferred"/>
  </effect>
  <effect name="PositionNormalUVUV2TechniqueDeferred" shader_family="PositionNormalUVUV2Deferred">
   <shader stage="vertex"   name="PositionNormalUVUV2Deferred"/>
   <shader stage="pixel"    name="PositionNormalUVUV2Deferred"/>
  </effect>
  <effect name="PositionBumpUVTechniqueDeferred" shader_family="PositionBumpUVDeferred">
   <shader stage="vertex"   name="PositionBumpUVDeferred"/>
   <shader stage="pixel"    name="PositionBumpUVDeferred"/>
  </effect>
  <effect name="AnimatedModelTechniqueDeferred"  shader_family="AnimatedModelDeferred">
	<shader stage="vertex" name="AnimatedModelDeferred"/>
	<shader stage="pixel" name="AnimatedModelDeferred"/>
  </effect>
  
  <!-- SHADOWS -->
  <effect name="PositionNormalShadowTechnique" shader_family="PositionNormalShadow">
   <shader stage="vertex"   name="PositionNormalShadow"/>
   <shader stage="pixel"    name="PositionNormalShadow"/>
  </effect>
  <effect name="PositionNormalUVShadowTechnique" shader_family="PositionNormalUVShadow">
   <shader stage="vertex"   name="PositionNormalUVShadow"/>
   <shader stage="pixel"    name="PositionNormalUVShadow"/>
  </effect>
  <effect name="PositionNormalUVUV2ShadowTechnique" shader_family="PositionNormalUVUV2Shadow">
   <shader stage="vertex"   name="PositionNormalUVUV2Shadow"/>
   <shader stage="pixel"    name="PositionNormalUVUV2Shadow"/>
  </effect>
  <effect name="PositionBumpUVShadowTechnique" shader_family="PositionBumpUVShadow">
   <shader stage="vertex"   name="PositionBumpUVShadow"/>
   <shader stage="pixel"    name="PositionBumpUVShadow"/>
  </effect>
  <effect name="AnimatedModelShadowTechnique"  shader_family="AnimatedModelShadow">
	<shader stage="vertex" name="AnimatedModelShadow"/>
	<shader stage="pixel" name="AnimatedModelShadow"/>
  </effect>
  
  
  <!-- POST PROCESS -->
  <effect name="NoiseVignettingTechnique" shader_family="NoiseVignetting">
   <shader stage="vertex"   name="NoiseVignetting"/>
   <shader stage="pixel"    name="NoiseVignetting"/>
  </effect>
  <effect name="FogTechnique" shader_family="Fog">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="Fog"/>
  </effect>
  <effect name="ZBlurTechnique" shader_family="ZBlur">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="ZBlur"/>
  </effect>
  <effect name="BloomTechnique" shader_family="Bloom">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="Bloom"/>
  </effect>
  <effect name="BloomCombineTechnique" shader_family="BloomCombine">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="BloomCombine"/>
  </effect>
  <effect name="GaussianTechnique" shader_family="Gaussian">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="Gaussian"/>
  </effect>  
  
  <effect name="HdaoTechnique" shader_family="Hdao">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="Hdao"/>
  </effect>  
  
  <effect name="ParticleVertexTechnique" shader_family="ParticleVertex">
   <shader stage="vertex"   name="ParticleVertex"/>
   <shader stage="pixel"    name="ParticleVertex"/>
   <shader stage="geometry" name="ParticleVertex"/>
  </effect>
  
 <effect name="ColorGradingTechnique" shader_family="ColorGrading">
	<shader stage="vertex"   name="Quad"/>
	<shader stage="pixel"    name="ColorGrading"/>
  </effect>  

  <effect name="SSRTechnique" shader_family="SSR">
	<shader stage="vertex"   name="Quad"/>
	<shader stage="pixel"    name="SSR"/>
  </effect>  

  <effect name="MascaraTechnique" shader_family="Mascara">
	<shader stage="vertex"   name="Quad"/>
	<shader stage="pixel"    name="Mascara"/>
  </effect>  
  
  <effect name="LightScatteringTechnique" shader_family="LightScattering">
	<shader stage="vertex"   name="Quad"/>
	<shader stage="pixel"    name="LightScattering"/>
  </effect>  
  
  <!-- OTHER -->
 <effect name="SkyboxTechnique" shader_family="Skybox">
   <shader stage="vertex"   name="Skybox"/>
   <shader stage="pixel"    name="Skybox"/>
  </effect>
  <effect name="QuadTechnique" shader_family="Quad">
   <shader stage="vertex"   name="Quad"/>
   <shader stage="pixel"    name="Quad"/>
  </effect>
  
  <effect name="DeferredQuadTechnique" shader_family="DeferredQuad">
   <shader stage="vertex"   name="DeferredQuad"/>
   <shader stage="pixel"    name="DeferredQuad"/>
  </effect>
    
 </effects>
 
 