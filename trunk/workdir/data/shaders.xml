<shaders path = "data/shaders/">
	<!--VERTEX SHADERS-->
	<vertex_shaders>
		<shader name="PositionNormalForward" file="Forward/PositionNormalLight.fx" entry_point="VS" vertex_type="PositionNormal" preprocessor=""/>
		<shader name="PositionNormalUVForward" file="Forward/PositionNormalUV.fx" entry_point="VS" vertex_type="PositionNormalUV" preprocessor=""/>
		<shader name="PositionNormalUVUV2Forward" file="Forward/PositionNormalUVUV2.fx" entry_point="VS" vertex_type="PositionNormalUVUV2" preprocessor=""/>
		<!--
		<shader name="PositionNormalUVUV2Forward" file="Forward/PositionNormalUVUV2.fx" entry_point="VS" vertex_type="PositionNormalUVUV2" preprocessor=""/>
		-->
		
		<shader name="AnimatedModelForward" file="Forward/AnimatedModel.vs" entry_point="mainVertexShader" vertex_type="PositionWeightIndicesNormalUV" preprocessor=""/>
	</vertex_shaders>     
	<!--PIXEL SHADERS-->  
	<pixel_shaders>
		<shader name="PositionNormalForward" file="Forward/PositionNormalLight.fx" entry_point="PS" preprocessor=""/>
		<shader name="PositionNormalUVForward" file="Forward/PositionNormalUV.fx" entry_point="PS" preprocessor=""/>		
		<shader name="PositionNormalUVUV2Forward" file="Forward/PositionNormalUVUV2.fx" entry_point="PS" preprocessor=""/>
		<!--
		<shader name="PositionNormalForward" file="Forward/RED.fx" entry_point="PS" preprocessor=""/>
		-->
		
		<shader name="AnimatedModelForward" file="Forward/AnimatedModel.ps" entry_point="mainPixelShader" preprocessor=""/>
	</pixel_shaders>
</shaders> 