<shaders path = "data/shaders/">
	<!--VERTEX SHADERS-->
	<vertex_shaders>
		
		<shader name="AnimatedModelForward" file="Forward/AnimatedModel.fx" entry_point="mainVertexShader" vertex_type="PositionWeightIndicesNormalUV" preprocessor=""/>
		<shader name="PositionNormalForward" file="Forward/PositionNormal.fx" entry_point="VS" vertex_type="PositionNormal" preprocessor=""/>
		<shader name="PositionNormalUVForward" file="Forward/PositionNormalUV.fx" entry_point="VS" vertex_type="PositionNormalUV" preprocessor=""/>
		<shader name="PositionNormalUVUV2Forward" file="Forward/PositionNormalUVUV2.fx" entry_point="VS" vertex_type="PositionNormalUVUV2" preprocessor=""/>
